https://leetcode.com/problems/number-of-1-bits/
class Solution {
    public int hammingWeight(int n) {
        String a="";
        int count=0;
        while(n>0){
            int g=n%2;
            count+=g;
            a+=Integer.toString(g);
            a/=2;
        }
        
         return g;
    }
}
