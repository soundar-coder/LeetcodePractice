//https://leetcode.com/problems/number-of-strings-that-appear-as-substrings-in-word/
class Solution {
    public int numOfStrings(String[] str, String word) {

        int count=0;
        for(int i=0;i<str.length;i++){
            if(word.contains(str[i])){
                count++;
            }
        }
        return count;
        
    }
}
